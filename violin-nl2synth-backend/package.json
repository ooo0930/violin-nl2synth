{
  "name": "violin-nl2synth-backend",
  "version": "1.0.0",
  "description": "Violin Natural Language to Synthesizer Backend",
  "main": "src/app.ts",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "ts-node src/app.ts",
    "dev:frontend": "cd ../violin-nl2synth-frontend && npm run dev",
    "open": "npm run dev & node scripts/open-browser.js",
    "test": "jest"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.28",
    "ajv": "^8.12.0",
    "axios": "^1.6.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.12",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "concurrently": "^7.5.0",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
